@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@using Rhino.Identity.Models.Account
@model RegisterModel

<form class="form-inline" method="post">
    <div>
        <label asp-for="Name" class="">Your name:</label>
        <input asp-for="Name" />
    </div>
    <div>
        <label asp-for="LastName" class="">Your last name:</label>
        <input asp-for="LastName" />
    </div>
    <div>
        <label asp-for="Email" class="">Your email:</label>
        <input asp-for="Email" />
    </div>
    <div>
        <label asp-for="Password" class="">Your password:</label>
        <input asp-for="Password" />
    </div>
    <div>
        <label asp-for="RepeatPassword" class="">Repeat your password:</label>
        <input asp-for="RepeatPassword" />
    </div>
    <div>
        <label asp-for="Age" class="">Your age:</label>
        <input asp-for="Age" />
    </div>
    <button type="submit" class="nav-link btn btn-link text-dark">Register</button>
</form>